package test1;

import java.io.FileNotFoundException;
import java.io.FileWriter;
import java.io.IOException;

public class output() extends Exception{
	FileWriter fw = null;
	
	try {
		fw= new FileWriter("test.txt");
		fw.write(97);
		fw.write(65);
		
	} catch(FileNotFoundException e) {
		e.printStackTrace();
	} catch(IOException e) {
		e.printStackTrace();
	} finally {
		fw.close();
	}
}
