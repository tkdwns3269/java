package test1;

import test240710.Person;

public class Test {
	public static void main(String[] args) {
		Person[] pArr = new Person[3];
		String[] arr = new String[3]; 
		arr[0] = "a";
		arr[1] = "b";
		arr[2] = "c";
		for(int i=0; i<=pArr.length; i++) {
			pArr[i] = arr[i];
			System.out.println(pArr[i].getName());
		}
	}
}
